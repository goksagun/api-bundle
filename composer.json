{
    "name": "goksagun/api-bundle",
    "description": "Api bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Burak Bolat",
            "email": "brkblt@gmail.com"
        }
    ],
    "license": "MIT",
    "keywords": [
        "symfony api bundle"
    ],
    "require": {
        "php": "^8.2",
        "symfony/validator": "*",
        "symfony/http-foundation": "*",
        "symfony/http-kernel": "*",
        "symfony/dependency-injection": "*",
        "symfony/framework-bundle": "*",
        "symfony/config": "*",
        "doctrine/orm": "*",
        "doctrine/annotations": "*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "*"
    },
    "autoload": {
        "psr-4": {
            "Goksagun\\ApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goksagun\\ApiBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
